{
  "name": "ngxmatfire",
  "version": "1.5.1",
  "author": "Jeroen Ouwehand",
  "description": "Full stack starter app with Angular 8, Material Design and Firebase.",
  "keywords": [
    "angular",
    "angular 8",
    "firebase",
    "material",
    "nosql",
    "oauth",
    "starter-template",
    "ngx"
  ],
  "repository": "https://github.com/jeroenouw/AngularMaterialFirebase",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reset": "rm -rf ./node_modules ./package-lock.json && npm install",
    "dist": "ng build",
    "prod": "ng build --prod",
    "prod:hashless": "ng build --prod --output-hashing none",
    "prod:src": "ng build --prod --sourcemaps",
    "compodoc:g": "compodoc -p src/tsconfig.app.json",
    "compodoc:s": "compodoc -p src/tsconfig.app.json --serve",
    "compodoc:w": "compodoc -p src/tsconfig.app.json --serve --watch",
    "tscov": "tscov -m 90",
    "tscov:d": "tscov --details",
    "link-upstream": "git remote add upstream https://github.com/jeroenouw/AngularMaterialFirebase",
    "sync": "git fetch && git checkout master && git merge upstream/master && git push"
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/fire": "^5.1.3",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/router": "^8.0.0",
    "core-js": "^3.1.3",
    "firebase": "^5.7.3",
    "hammerjs": "^2.0.8",
    "ngx-auth-firebaseui": "^2.2.1",
    "rxjs": "^6.5.0",
    "rxjs-compat": "^6.5.0",
    "ts-helpers": "*",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@liftr/tscov": "^1.4.1",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^11.10.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.1",
    "webpack": "^4.29.6"
  },
  "peerDependencies": {
    "typescript": "3.4.5"
  }
}
